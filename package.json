{
  "name": "backpack",
  "version": "0.0.1",
  "description": "Backpack Design System",
  "scripts": {
    "clean:dist": "rm -rf dist",
    "clean": "npm run clean:dist",
    "lint:scss": "sass-lint -v",
    "lint:js": "eslint . --ext .jsx --ext .js",
    "lint:fix": "eslint . --ext .jsx --ext .js --fix",
    "lint": "npm run lint:js && npm run lint:scss",
    "sassdoc": "sassdoc {scss/**/*,scss/*,node_modules/backpack-tokens/tokens/base}.scss -d dist/sassdoc -v --strict",
    "dist": "NODE_ENV=production npm run clean && webpack && npm run sassdoc",
    "dist:gzip": "npm run dist && grunt",
    "start": "webpack-dev-server",
    "test": "npm run lint"
  },
  "main": "_backpack.scss",
  "repository": {
    "type": "git",
    "url": "git@git.prod.skyscanner.local:backpack/backpack.git"
  },
  "author": "Backpack Design System <backpack@skyscanner.net>",
  "license": "Apache-2.0",
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "babel-core": "6.9.1",
    "babel-loader": "6.2.4",
    "babel-plugin-syntax-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "bpk-component-button": "0.0.6-alpha.d2203521",
    "css-loader": "0.23.1",
    "eslint-config-standard": "5.3.1",
    "eslint-config-standard-jsx": "1.2.1",
    "eslint-config-standard-react": "2.4.0",
    "eslint-plugin-promise": "1.3.2",
    "eslint-plugin-react": "5.2.2",
    "eslint-plugin-standard": "1.3.2",
    "extract-text-webpack-plugin": "1.0.1",
    "grunt": "0.4.5",
    "grunt-cli": "0.1.13",
    "grunt-contrib-compress": "1.3.0",
    "js-beautify": "1.6.3",
    "json-loader": "0.5.4",
    "lodash": "4.13.1",
    "node-sass": "3.8.0",
    "postcss-loader": "0.9.1",
    "react": "15.1.0",
    "react-css-modules": "3.7.6",
    "react-dom": "15.1.0",
    "react-helmet": "3.1.0",
    "react-router": "2.4.1",
    "sass-lint": "1.8.2",
    "sass-loader": "3.2.1",
    "sassdoc": "2.1.20",
    "standard": "7.1.2",
    "static-site-generator-webpack-plugin": "2.1.0",
    "style-loader": "0.13.1",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "backpack-tokens": "git+http://git.prod.skyscanner.local/backpack/tokens.git#v6.0.0",
    "normalize.css": "4.1.1"
  }
}
