{
  "name": "backpack",
  "private": true,
  "description": "Backpack Design System",
  "scripts": {
    "clean:dist": "rm -rf dist",
    "clean": "npm run clean:dist",
    "lint:scss": "stylelint docs/**/*.scss *.scss --syntax scss",
    "lint:js": "standard",
    "lint": "npm run lint:js && npm run lint:scss",
    "sassdoc": "sassdoc {node_modules/bpk-mixins/src/**/*,node_modules/backpack-tokens/tokens/base.default}.scss -d dist/sassdoc -v --strict --debug",
    "dist": "npm run clean && NODE_ENV=production webpack && npm run sassdoc",
    "dist:gzip": "npm run dist && grunt",
    "start": "webpack-dev-server",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git@git.prod.skyscanner.local:backpack/backpack.git"
  },
  "author": "Backpack Design System <backpack@skyscanner.net>",
  "license": "Apache-2.0",
  "devDependencies": {
    "autoprefixer": "6.3.7",
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-plugin-syntax-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "backpack-tokens": "git+http://git.prod.skyscanner.local/backpack/tokens.git#v6.0.0",
    "bpk-component-button": "*",
    "bpk-component-heading": "*",
    "bpk-component-list": "*",
    "bpk-component-paragraph": "*",
    "bpk-component-table": "*",
    "bpk-mixins": "*",
    "bpk-stylesheets": "*",
    "css-loader": "0.23.1",
    "extract-text-webpack-plugin": "1.0.1",
    "grunt": "0.4.5",
    "grunt-cli": "0.1.13",
    "grunt-contrib-compress": "1.3.0",
    "js-beautify": "1.6.3",
    "json-loader": "0.5.4",
    "lodash": "4.13.1",
    "node-sass": "3.8.0",
    "postcss-loader": "0.9.1",
    "react": "15.2.1",
    "react-css-modules": "3.7.7",
    "react-dom": "15.2.1",
    "react-helmet": "3.1.0",
    "react-router": "2.5.2",
    "sass-loader": "4.0.0",
    "sassdoc": "2.1.20",
    "standard": "7.1.2",
    "static-site-generator-webpack-plugin": "2.1.0",
    "style-loader": "0.13.1",
    "stylelint": "7.0.2",
    "stylelint-config-standard": "11.0.0",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  }
}
