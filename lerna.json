{
  "version": "independent",
  "packages": ["packages/*"],
  "command": {
    "bootstrap": {
      "ignore": ["bpk-tether"]
    },
    "publish": {
      "ignoreChanges": [
        "bpk-tether",
        "package-lock.json",
        "UNRELEASED.yaml",
        "CHANGELOG.md",
        "**/package.json",
        "**package-lock.json"
      ],
      "allowBranch": "master"
    }
  }
}
