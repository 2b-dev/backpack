{
  "name": "bpk-component-icon",
  "version": "3.3.0",
  "description": "Backpack icon components.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./sm && rm -rf ./lg && mkdir ./sm  && mkdir ./lg",
    "copy:sm": "cp ./node_modules/bpk-svgs/dist/js/icons/sm/*.js ./sm",
    "copy:lg": "cp ./node_modules/bpk-svgs/dist/js/icons/lg/*.js ./lg",
    "build": "npm run clean && npm run copy:sm && npm run copy:lg"
  },
  "repository": {
    "type": "git",
    "url": "git@git.prod.skyscanner.local:backpack/backpack.git"
  },
  "author": "Backpack Design System <backpack@skyscanner.net>",
  "license": "Apache-2.0",
  "peerDependencies": {
    "react": "^15.0.0"
  },
  "dependencies": {
    "bpk-mixins": "^8.3.0",
    "bpk-svgs": "^4.3.0",
    "bpk-tokens": "^17.0.6"
  }
}
